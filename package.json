{
  "name": "config",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test-with-report": "jest --coverage --coverage --reporters=default --reporters=jest-junit --watchAll=false -w 1",
    "eject": "react-scripts eject",
    "check-ts": "yarn tsc --noUnusedLocals --noUnusedParameters",
    "check-format": "prettier --check src",
    "check-lint": "cross-env STRICT_MODE=TRUE eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.14",
    "eslint-config-next": "13.3.1",
    "next": "13.3.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",

    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "antd": "^4.24.8",
    "axios": "^1.2.1",
    "eslint": "8.35.0",
    "react-query": "^3.39.3",
    "react-scripts": "5.0.1",
    "sass": "^1.58.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/react": "18.0.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "cross-env": "^7.0.3",
    "eslint-plugin-etc": "^2.0.2",
    "husky": "^8.0.3",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-junit": "^15.0.0",
    "typescript": "4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@shotgunjed)/"
    ],
    "coveragePathIgnorePatterns": [
      "src/service/mockdata",
      "src/config.ts",
      "src/setupTests.ts"
    ]
  }
}
